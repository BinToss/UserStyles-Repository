{
  "eslint.validate": [
    "css",
    "javascript",
    "json",
    "json5",
    "jsonc",
    "markdown",
    "typescript"
  ],
  "eslint.probe": [
    "css",
    "javascript",
    "json",
    "json5",
    "jsonc",
    "markdown",
    "typescript"
  ],
  "eslint.format.enable": true,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "eslint.nodePath": ".yarn/sdks",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true
}